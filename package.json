{
  "name": "aliencyborg",
  "version": "1.0.0",
  "private": true,
  "description": "Static ember-based homepage for aliencyb.org",
  "repository": "https://github.com/aliencyborg/homepage.git",
  "license": "MIT",
  "author": "Benjamin Rosas <ben@aliencyb.org>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "slowboot": "FASTBOOT_DISABLED=true ember serve",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
